{
	"name": "pricing",
	"version": "1.2.0",
	"engines": {
		"node": ">=10.0.0"
	},
	"author": "Gaspard Bariller <gaspard.bariller@tutanota.com>",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "parcel watch \"src/web/index.html\" --dist-dir \"dev/\" --cache-dir \".cache/parcel/\"",
		"style": "prettier --write \"**/*.{html,xhtml,css,scss,md,xml,yaml,yml,json,js}\"",
		"test": "nyc mocha",
		"lint-js": "eslint --fix --cache --cache-location \".cache/eslint/\" \"**/*.js\"",
		"lint-css": "stylelint \"**/*.{css,scss}\" --fix",
		"build": "parcel build \"src/web/index.html\" --dist-dir \"dist/web/\" --public-url \".\" --cache-dir \".cache/parcel/\""
	},
	"browserslist": [
		"last 2 versions",
		"Firefox ESR",
		"IE 11",
		"not dead"
	],
	"dependencies": {
		"chart.js": "3.0.0-beta",
		"gridjs": "1.17.0",
		"spectre.css": "0.5.9",
		"zipcelx": "1.6.2"
	},
	"devDependencies": {
		"@prettier/plugin-xml": "0.12.0",
		"chai": "4.2.0",
		"eslint": "7.11.0",
		"eslint-config-prettier": "6.13.0",
		"mocha": "8.2.0",
		"nyc": "15.1.0",
		"parcel": "2.0.0-beta.1",
		"prettier": "2.1.2",
		"sass": "1.28.0",
		"stylelint": "13.7.2",
		"stylelint-config-prettier": "8.0.2",
		"stylelint-config-sass-guidelines": "7.1.0"
	}
}
