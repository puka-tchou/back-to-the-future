{
	"name": "pricing",
	"version": "1.4.0",
	"engines": {
		"node": ">=10.0.0"
	},
	"author": "Gaspard Bariller <gaspard.bariller@tutanota.com>",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "parcel watch \"src/web/index.html\" --dist-dir \"dev/\" --cache-dir \".cache/parcel/\"",
		"style": "prettier --write \"**/*.{html,xhtml,css,scss,md,xml,yaml,yml,json,js}\"",
		"test": "echo 'These tests are not implemented yet.' && echo Lines: 0.0%",
		"lint-js": "tsc --noemit && eslint --fix --cache --cache-location \".cache/eslint/\" \"**/*.{js,ts}\"",
		"lint-css": "stylelint \"**/*.{css,scss}\" --fix",
		"build": "parcel build \"src/web/index.html\" --dist-dir \"dist/www/\" --public-url \".\" --cache-dir \".cache/parcel/\""
	},
	"browserslist": [
		"last 2 versions",
		"Firefox ESR",
		"IE 11",
		"not dead"
	],
	"dependencies": {
		"chart.js": "4.1.2",
		"gridjs": "5.1.0",
		"slim-select": "2.3.3",
		"spectre.css": "0.5.9",
		"zipcelx": "1.6.2"
	},
	"devDependencies": {
		"@parcel/config-default": "2.8.2",
		"@parcel/transformer-typescript-tsc": "2.8.2",
		"@parcel/validator-typescript": "2.8.2",
		"@prettier/plugin-xml": "2.2.0",
		"@types/chart.js": "2.9.37",
		"@types/zipcelx": "1.5.0",
		"@typescript-eslint/eslint-plugin": "5.48.0",
		"@typescript-eslint/parser": "5.48.0",
		"chai": "4.3.7",
		"eslint": "8.31.0",
		"eslint-config-prettier": "8.6.0",
		"mocha": "10.2.0",
		"nyc": "15.1.0",
		"parcel": "2.8.2",
		"prettier": "2.8.1",
		"sass": "1.57.1",
		"stylelint": "14.16.1",
		"stylelint-config-prettier": "9.0.4",
		"stylelint-config-sass-guidelines": "9.0.1",
		"typescript": "4.9.4"
	}
}
